package org.teachingkidsprogramming.CodingChallenges;

public class TenIntegers
{
  public static void main(String[] args)
  {
    for (int i = 0; i < 10; i++)
    {
      System.out.println(i);
    }
    System.out.println(0);
    for (int i = 1; i < 11; i++)
    {
      System.out.println(i);
    }
  }
}
