package org.teachingkidsprogramming.CodingChallenges;

public class TenRowsOfTen
{
  public static void main(String[] args)
  {
    for (int i = 0; i < 11; i++)
    {
      System.out.print((i + 1) + " ");
      System.out.println();
      for (int j = 0; j < 101; j++)
      {
        System.out.print(j + 1);
      }
    }
  }
}
