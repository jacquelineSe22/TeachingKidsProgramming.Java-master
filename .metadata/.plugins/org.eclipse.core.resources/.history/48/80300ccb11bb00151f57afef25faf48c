package org.teachingkidsprogramming.CodingChallenges;

public class TenRows
{
  public static void main(String[] args)
  {
    for (int i = 0; i < 11; i++)
    {
      System.out.print(i);
      System.out.println();
      for (int j = 11; j < 10; j++)
      {
        System.out.print(j);
      }
    }
  }
}
